		{
			"eventId": "EVENT_BEFORESHOW_MENU",
			"pageType": "menu",
			"code": "global $conn;\n\n// Workorders Summary\n$years = []; $year_totals = [];\n$rs = DB::Query(\"SELECT year, SUM(total_workorders) as total FROM workorders_summary GROUP BY year ORDER BY year DESC\");\nwhile($data = $rs->fetchAssoc()) {\n    $years[] = $data[\"year\"];\n    $year_totals[] = $data[\"total\"];\n}\n\n// Top 5 staff\n$staffNames = []; $staffCounts = [];\n$rs = DB::Query(\"SELECT WOreqBy, SUM(total_workorders) as total FROM workorders_by_staff GROUP BY WOreqBy ORDER BY total DESC LIMIT 5\");\nwhile($data = $rs->fetchAssoc()) {\n    $staffNames[] = $data[\"WOreqBy\"];\n    $staffCounts[] = $data[\"total\"];\n}\n\n// Parts used\n$partNames = []; $partCounts = [];\n$rs = DB::Query(\"SELECT partsUsed, SUM(usage_count) as count FROM parts_usage_summary GROUP BY partsUsed ORDER BY count DESC LIMIT 5\");\nwhile($data = $rs->fetchAssoc()) {\n    $partNames[] = $data[\"partsUsed\"];\n    $partCounts[] = $data[\"count\"];\n}\n\n// Status distribution\n$statusLabels = []; $statusCounts = [];\n$rs = DB::Query(\"SELECT status, SUM(total) as total FROM status_distribution GROUP BY status\");\nwhile($data = $rs->fetchAssoc()) {\n    $statusLabels[] = $data[\"status\"];\n    $statusCounts[] = $data[\"total\"];\n}\n\n// Downtime summary\n$downtimeLabels = []; $downtimeValues = [];\n$rs = DB::Query(\"SELECT year, AVG(avg_downtime_days) as avg_downtime FROM downtime_summary GROUP BY year ORDER BY year\");\nwhile($data = $rs->fetchAssoc()) {\n    $downtimeLabels[] = $data[\"year\"];\n    $downtimeValues[] = round($data[\"avg_downtime\"], 2);\n}\n\n// Send JS data to HTML Snippet\necho \"\n<script>\nconst years = \" . json_encode($years) . \";\nconst yearTotals = \" . json_encode($year_totals) . \";\nconst staffLabels = \" . json_encode($staffNames) . \";\nconst staffData = \" . json_encode($staffCounts) . \";\nconst partsLabels = \" . json_encode($partNames) . \";\nconst partsData = \" . json_encode($partCounts) . \";\nconst statusLabels = \" . json_encode($statusLabels) . \";\nconst statusData = \" . json_encode($statusCounts) . \";\nconst downtimeLabels = \" . json_encode($downtimeLabels) . \";\nconst downtimeData = \" . json_encode($downtimeValues) . \";\n</script>\";\n"
		},
		{
			"eventId": "EVENT_JS_ONLOAD",
			"pageType": "menu",
			"code": "// Load Chart.js if not already loaded\nlet chartScript = document.createElement('script');\nchartScript.src = \"https://cdn.jsdelivr.net/npm/chart.js\";\ndocument.head.appendChild(chartScript);\n\n// Wait for Chart.js to load, then render charts\nchartScript.onload = function () {\n    // Workorders\n    new Chart(document.getElementById('workordersChart'), {\n        type: 'bar',\n        data: {\n            labels: years,\n            datasets: [{\n                label: 'Total Work Orders',\n                data: yearTotals,\n                backgroundColor: 'rgba(13,110,253,0.7)'\n            }]\n        }\n    });\n\n    // Staff\n    new Chart(document.getElementById('staffChart'), {\n        type: 'bar',\n        data: {\n            labels: staffLabels,\n            datasets: [{\n                label: 'Requests by Staff',\n                data: staffData,\n                backgroundColor: 'rgba(25,135,84,0.7)'\n            }]\n        }\n    });\n\n    // Parts\n    new Chart(document.getElementById('partsChart'), {\n        type: 'pie',\n        data: {\n            labels: partsLabels,\n            datasets: [{\n                data: partsData,\n                backgroundColor: ['#ffc107', '#fd7e14', '#198754', '#0dcaf0', '#6f42c1']\n            }]\n        }\n    });\n\n    // Status\n    new Chart(document.getElementById('statusChart'), {\n        type: 'doughnut',\n        data: {\n            labels: statusLabels,\n            datasets: [{\n                data: statusData,\n                backgroundColor: ['#0d6efd', '#dc3545', '#ffc107', '#198754', '#6c757d']\n            }]\n        }\n    });\n\n    // Downtime\n    new Chart(document.getElementById('downtimeChart'), {\n        type: 'line',\n        data: {\n            labels: downtimeLabels,\n            datasets: [{\n                label: 'Avg Downtime (Days)',\n                data: downtimeData,\n                borderColor: 'rgba(220,53,69,0.9)',\n                tension: 0.4\n            }]\n        }\n    });\n};\n"
		}
